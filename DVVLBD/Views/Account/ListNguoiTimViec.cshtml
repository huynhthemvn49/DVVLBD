<table class="table table-striped table-bordered table-hover ">
    <thead>
        <tr>
            <th>NTVid</th>
            <th>Họ tên</th>
            <th>Tên Hồ sơ</th>
            <th>Lượt xem</th>
            <th>Chi tiết/Xóa</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.NguoiTimViec != null)
        {

            foreach (DVVLBD.Models.dvvlBD.NguoiTimViec_HS item in ViewBag.NguoiTimViec)
            {
                <tr>
                    <td>@item.ID_HSXinViec</td>
                    <td>@item.NguoiTimViec.HoTen</td>
                    <td>@item.TenHSXinViec</td>
                    <td>@item.LuotXem</td>
                    <td class="center">
                        <a class="btn btn-danger" style="color: #fff; background-color: #d9534f; border-color: #d43f3a;" href="/NTV/NTV_Detail/@item.ID_HSXinViec" target="_blank"><span class="glyphicon glyphicon-edit"></span></a>
                        <a class="btn btn-warning" onclick="return btnXoaHSXV()" style="color: #fff; background-color: #f0ad4e; border-color: #eea236;" @*href="/Account/XoaHSTD/@item.ID_HSTuyenDung"*@><span class="glyphicon glyphicon-trash"></span></a>
                    </td>
                </tr>
            }
        }
    </tbody>

</table>